<!-- 导航 -->

<div class="container-fluid">
    <form class="form-inline m_b10">
        <div class="form-group m_r60">
            <span style="float:left;">&nbsp&nbsp&nbsp*UM_ID：</span>
            <input id="UM_ID" maxlength="20">
        </div>
    </form>
    <form class="form-inline m_b10">
        <div class="form-group m_r60">
            <span style="float:left;">*用户姓名：</span>
            <input id="userName" maxlength="10">
        </div>
    </form>
    <form class="form-inline m_b10">
        <div class="form-group m_r60">
            <span style="float:left;">&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp&nbsp状态：</span>
            <label class="radio-inline">
                <input type="radio" name="Status" id="inlineRadio1" value="1" checked="true"> 启用
            </label>
            <label class="radio-inline">
                <input type="radio" name="Status" id="inlineRadio2" value="0"> 禁用
            </label>
        </div>
    </form>
</div>
